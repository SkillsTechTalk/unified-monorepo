#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules/tailwindcss/lib/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules/tailwindcss/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules/tailwindcss/lib/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules/tailwindcss/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/tailwindcss@2.2.19_autoprefixer@10.4.21_postcss@8.5.6__postcss@8.5.6_ts-node@10.9.2_@ty_9184ea864cd438624396703bd0fbf57c/node_modules:/Users/cmutaba/Dev/unified-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
